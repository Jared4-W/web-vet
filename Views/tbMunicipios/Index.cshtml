@model IEnumerable<Examen1_JaredChavez.Models.tbMunicipios>

@{
    ViewBag.Title = "Index";
}

<h2>municipios</h2>

<p>
    @Html.ActionLink("Registrar municipio", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Municipio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbEstados.Estado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Municipio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbEstados.Estado)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IdMunicipio }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.IdMunicipio }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IdMunicipio })
        </td>
    </tr>
}

</table>
