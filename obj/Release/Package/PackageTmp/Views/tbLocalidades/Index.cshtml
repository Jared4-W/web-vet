@model IEnumerable<Examen1_JaredChavez.Models.tbLocalidades>

@{
    ViewBag.Title = "Index";
}

<h2>Localidades</h2>

<p>
    @Html.ActionLink("Registrar localidad", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Localidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbMunicipios.Municipio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Localidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbMunicipios.Municipio)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IdLocalidad }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.IdLocalidad }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IdLocalidad })
        </td>
    </tr>
}

</table>
